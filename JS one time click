<body>
<button id="clic">click</button>
<script>
    let onc=document.getElementById("clic");
    let count=0
    onc.addEventListener("click",onetime,{once:true})
    function onetime(){
        count+=1;
        console.log("count",count)


    }
</script>
 
    
</body>
